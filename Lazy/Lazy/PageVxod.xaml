<?xml version="1.0" encoding="utf-8" ?>
<ContentPage xmlns="http://xamarin.com/schemas/2014/forms"
             xmlns:x="http://schemas.microsoft.com/winfx/2009/xaml"
             x:Class="Lazy.PageVzod">
    <ContentPage.Content>
        <StackLayout>
            <Label Text="Авторизация"/>
            <Entry x:Name="Number_phone" Placeholder="Номер телефона"/>
            <Entry x:Name="Password" Placeholder="Пароль" IsPassword="True"/>
            <CheckBox x:Name="CheckPassword"/>
            <Button x:Name="Vxod" Text="Далее" Clicked="Vxod_Clicked"/>
        </StackLayout>
    </ContentPage.Content>
</ContentPage>